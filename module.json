{
  "id": "effectivetray",
  "title": "Effective Tray",
  "description": "Enhances the D&D5e effect and damage trays to allow players to apply effects and damage to targets they don’t own—with GM-controlled permissions. Includes socket-based GM delegation, filtering, concentration support, and macro helpers.",
  "version": "13.350.0.0",
  "compatibility": {
    "minimum": "13",
    "verified": "13.350",
    "maximum": "13.999"
  },
  "esmodules": [
    "scripts/libwrapper-shim/shim.js",
    "scripts/setup.mjs"
  ],
  "styles": [
    "styles/effectivetray.css"
  ],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "compatibility": {
          "minimum": "5.1.0",
          "verified": "5.1.10",
          "maximum": "5.1.9999"
        }
      }
    ],
    "recommends": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "compatibility": {
          "minimum": "1.13.0.0",
          "verified": "1.13.4.0"
        }
      }
    ]
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "flags": {
    "hotReload": {
      "extensions": ["css", "json"],
      "paths": ["styles/effectivetray.css", "lang"]
    }
  },
  "socket": true,
  "url": "https://github.com/perlygatekeeper/effectivetray",
  "bugs": "https://github.com/perlygatekeeper/effectivetray/issues",
  "changelog": "https://github.com/perlygatekeeper/effectivetray/releases/latest",
  "download": "https://github.com/perlygatekeeper/effectivetray/archive/refs/heads/main.zip",
  "manifest": "https://github.com/perlygatekeeper/effectivetray/raw/main/module.json",
  "license": "LICENSE",
  "readme": "README.md",
  "authors": [
    {
      "name": "perlygatekeeper",
      "url": "https://github.com/perlygatekeeper"
    }
  ]
}
